package natasha_20170924;

public class NatashaHelp {
	public static void main(String[] args){
		
		System.out.println(isLeapYear(1900));
	}
	
	static boolean isLeapYear(int y){
		if (y%4 == 0 && y%100 != 0){
			return true;
		}else if (y%100 == 0 && y%400 == 0)
			return true;
		else{
			return false;
		}
	}
}
